```cpp
class Solution {
public:
    vector<int> successfulPairs(vector<int>& s, vector<int>& p, long long su) {
        vector<int> res;
        sort(p.begin(),p.end());
        for(int S:s){
            long long need=ceil((double)su/S);
            int pos=lower_bound(p.begin(),p.end(),need)-p.begin();
            int cnt = p.size() - pos;
            res.push_back(cnt);
        }
        return res;
    }
};
```
